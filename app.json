{
  "expo": {
    "name": "getoutv1",
    "slug": "getoutv1",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "getoutv1",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundlerIdentifier": "com.getoutbcn.app",
      "supportsTablet": true
    },
    "android": {
      "package": "com.getoutbcn.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4d42bf57-4731-4445-a54c-7a80a92556b1"
      }
    }
  }
}
